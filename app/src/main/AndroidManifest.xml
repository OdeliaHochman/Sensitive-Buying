<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sensitivebuying">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".ui.OpenActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.customer.HostNavigationActivity" />
        <activity android:name=".ui.customer.CustomerDetailsActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.customer.RegisterActivity" />
        <activity android:name=".ui.customer.SensitiveRegisterActivity" />
        <activity android:name=".ui.represntative.RepresentativeStatisticsActivity" />
        <activity android:name=".ui.represntative.RepresentativeProductDetailsActivity" />
        <activity android:name=".ui.represntative.RepresentativeAddProductActivity" />
        <activity android:name=".ui.represntative.RepresentativeSearchActivity" />
        <activity android:name=".ui.represntative.RepresentativeHomeActivity">
        </activity>
        <activity android:name=".ui.represntative.RepresentativeContactUsActivity" />
    </application>

</manifest>